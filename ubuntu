#!/usr/bin/env bash
if [[ -z $STOW_FOLDERS ]]; then
    STOW_FOLDERS="alacritty,atuin,aws,bash,bin,blesh,butterfish,clangd,docker,gradle,fusuma,kube,k9s,lazydocker,lazygit,netrc,mcphub,nvim,rclone,ssh,starship,tmux,testcontainers,vectorcode,wireplumber,yazi"
fi

if [[ -z $DOTFILES ]]; then
    DOTFILES=$HOME/Privatespace/.dotfiles
fi

STOW_FOLDERS=$STOW_FOLDERS DOTFILES=$DOTFILES $DOTFILES/install
